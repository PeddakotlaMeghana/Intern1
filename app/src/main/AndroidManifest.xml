<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.meghanapeddakotla.intern1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="MainActivity"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="Login Page">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PasswordActivity"
            android:label="Forgot Password"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".RegistrationActivity"
            android:label="Registration Page"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".HomeActivity"
            android:label="Home Page" />
        <activity
            android:name=".AddsessionActivity"
            android:label="Add Session"
            android:parentActivityName=".HomeActivity"/>

        <activity
            android:name=".EventsActivity"
            android:label="Events"
            android:parentActivityName=".AddsessionActivity"/>

        <activity
            android:name=".DetailsActivity"
            android:label="Details"
            android:parentActivityName=".EventsActivity"></activity>
    </application>

</manifest>